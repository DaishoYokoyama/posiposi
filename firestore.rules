service cloud.firestore {
  match /databases/{database}/documents/rooms {
    match /{document=**} {
      allow read, write;
    }
  }
  match /databases/{database}/documents/rooms/{document}/objects {
    match /{document=**} {
      allow read, write;
    }
  }
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
